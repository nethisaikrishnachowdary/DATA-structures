#define MAX 20
#include<stdio.h>
#include<stdlib.h>
int a[MAX],front=-1,rear=-1;
void enqueue(int x)
{
    if(rear == MAX-1)
        printf("queue overflow");
    else
    {
        if(front == -1)
            front=rear=0;
        else
            ++rear;
        a[rear]=x;
    }
}
int dequeue()
{
    int x;
    if(front == -1)
        return -1;
    else
    {
        x=a[front];
        if(front == rear)
            front=rear=-1;
        else
            ++front;
        return x;
    }
}
void display()
{
    int i;
    if(front == -1)
        printf("no elements in queue");
    else
    {
        for(i=front;i<=rear;i++)
            printf("%d\t",a[i]);
    }
}
int main()
{
    int ch,x;
    while(1)
    {
        printf("\n1.insert\n2.delete\n3.display\n4.exit\n");
        printf("enter your choice\n");
        scanf("%d",&ch);
        switch(ch)
        {
            case 1: printf("enter the element\n");
                    scanf("%d",&x);
                    enqueue(x);
                    break;
            case 2: x=dequeue();
                    if(x==-1)
                        printf("stack underflow\n");
                    else
                        printf("element deleted is %d\n",x);
                    break;
            case 3: printf("the elements in the queue are ");
                    display();
                    break;
            case 4: exit(0);
            default: printf("invalid choice");
        }
    }
    return 0;
}
